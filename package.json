{
    "name": "anonymous-file-upload-service",
    "version": "1.0.0",
    "description": "Anonymous file upload service with proxy to external storage",
    "main": "app.ts",
    "type": "module",
    "scripts": {
        "start": "node dist/app.js",
        "dev": "nodemon dist/app.js",
        "start:dev": "tsx app.ts",
        "build": "tsc",
        "watch": "tsc -w",
        "lint": "eslint .",
        "dev:ts": "nodemon --watch 'src/**' --ext 'ts,json' --exec 'ts-node src/app.ts'"
    },
    "dependencies": {
        "@types/dotenv": "^6.1.1",
        "@types/yamljs": "^0.2.34",
        "argon2": "^0.41.1",
        "axios": "^1.6.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "csurf": "^1.11.0",
        "dotenv": "^16.4.7",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.3",
        "form-data": "^4.0.0",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "minio": "^8.0.5",
        "multer": "^1.4.5-lts.2",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.23.0",
        "@types/cookie-parser": "^1.4.6",
        "@types/cors": "^2.8.17",
        "@types/csurf": "^1.11.5",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^20.11.0",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.6",
        "@types/uuid": "^9.0.7",
        "eslint": "^9.23.0",
        "globals": "^16.0.0",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.2",
        "tsx": "^4.19.3",
        "typescript": "^5.3.3",
        "typescript-eslint": "^8.27.0"
    }
}
